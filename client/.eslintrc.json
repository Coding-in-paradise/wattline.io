{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint-config-prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": ["error", "double"],
        "space-infix-ops": ["error", { "int32Hint": true }]
    }
}
